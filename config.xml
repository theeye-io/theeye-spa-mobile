        <preference name="BackupWebStorage" value="local" />
        <edit-config file="theEye Mobile/*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>TheEye allows you to take photos of your documents and attach them to a workflow</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>You can attach a photo or document to a workflow</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string>You can attach a photo or document to a workflow</string>
        </edit-config>
